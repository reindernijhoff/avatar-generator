{
  "name": "eigen-avatar-generator",
  "version": "1.1.2",
  "description": "Generative avatar generator with multiple themes. Generates unique, deterministic avatars for any ID.",
  "keywords": [
    "generative art",
    "generative design",
    "procedural generation",
    "javascript",
    "art",
    "code",
    "drawing",
    "creative coding",
    "avatar generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/reindernijhoff/avatar-generator.git"
  },
  "author": "Reinder Nijhoff <reinder@infi.nl>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./themes/digidoodle": {
      "types": "./dist/themes/digidoodle/index.d.ts",
      "import": "./dist/themes/digidoodle/index.js"
    },
    "./themes/interference": {
      "types": "./dist/themes/interference/index.d.ts",
      "import": "./dist/themes/interference/index.js"
    },
    "./themes/plasma": {
      "types": "./dist/themes/plasma/index.d.ts",
      "import": "./dist/themes/plasma/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./react/digidoodle": {
      "types": "./dist/react/AvatarDigiDoodle.d.ts",
      "import": "./dist/react/AvatarDigiDoodle.js"
    },
    "./react/interference": {
      "types": "./dist/react/AvatarInterference.d.ts",
      "import": "./dist/react/AvatarInterference.js"
    },
    "./react/plasma": {
      "types": "./dist/react/AvatarPlasma.d.ts",
      "import": "./dist/react/AvatarPlasma.js"
    }
  },
  "peerDependencies": {
    "canvas": "^2.11.2",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "canvas": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "npm run dev --prefix example",
    "build": "vite build",
    "clean": "shx rm -rf dist node_modules/.cache/.tsbuildinfo",
    "prepare-publish": "tsx ./scripts/preparePublish.ts",
    "example": "npm run dev --prefix example"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/shelljs": "^0.8.15",
    "react": "^18.3.1",
    "shelljs": "^0.10.0",
    "shx": "^0.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
