{
  "name": "eigen-avatar-generator",
  "version": "1.0.1",
  "description": "Generative avatar generator with multiple themes. Generates unique, deterministic avatars for any ID.",
  "keywords": [
    "generative art",
    "generative design",
    "procedural generation",
    "javascript",
    "art",
    "code",
    "drawing",
    "creative coding",
    "avatar generator"
  ],
  "repository": "git@github.com:reindernijhoff/avatar-generator.git",
  "author": "Reinder Nijhoff <reinder@infi.nl>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "main": "./dist/eigen-avatar-generator.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/eigen-avatar-generator.umd.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./themes/digidoodle": {
      "types": "./dist/themes/digidoodle/index.d.ts",
      "import": "./dist/themes/digidoodle/index.js"
    },
    "./themes/interference": {
      "types": "./dist/themes/interference/index.d.ts",
      "import": "./dist/themes/interference/index.js"
    }
  },
  "peerDependencies": {
    "canvas": "^2.11.2"
  },
  "peerDependenciesMeta": {
    "canvas": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "npm run dev --prefix example",
    "build": "vite build",
    "clean": "shx rm -rf dist node_modules/.cache/.tsbuildinfo",
    "prepare-publish": "tsx ./scripts/preparePublish.ts",
    "example": "npm run dev --prefix example"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.15",
    "shelljs": "^0.10.0",
    "shx": "^0.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
